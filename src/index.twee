:: StoryStylesheet [stylesheet]
/* === CONFIGURAÇÕES GERAIS === */
html {
  background-color: #0a0a0a;
  background-image: 
    repeating-linear-gradient(
      0deg,
      transparent,
      transparent 2px,
      rgba(0, 255, 100, 0.03) 2px,
      rgba(0, 255, 100, 0.03) 4px
    );
  animation: flicker 0.15s infinite;
}

body {
  background-color: transparent;
}

tw-story {
  background-color: #000000;
  color: #00ff41;
  font-family: 'Courier New', monospace;
  font-size: 16px;
  line-height: 1.6;
  padding: 40px;
  max-width: 800px;
  margin: 0 auto;
  border: 2px solid #00ff41;
  box-shadow: 
    0 0 10px rgba(0, 255, 65, 0.5),
    inset 0 0 100px rgba(0, 255, 65, 0.05);
  text-shadow: 0 0 5px rgba(0, 255, 65, 0.7);
}


/* === REPOSICIONAR BOTÃO UNDO === */
tw-sidebar {
  left: 630px !important;
  top: -60px !important;
  display: flex !important;
  flex-direction: row !important;
  gap: 10px !important;
}


/* === ANIMAÇÃO DE FLICKER === */
@keyframes flicker {
  0% { opacity: 0.97; }
  50% { opacity: 1; }
  100% { opacity: 0.97; }
}

/* === LINKS === */
tw-link {
  color: #00ff41;
  text-decoration: none;
  border-bottom: 1px solid #00ff41;
  transition: all 0.2s;
  cursor: pointer;
}

tw-link:hover {
  color: #ffffff;
  border-bottom-color: #ffffff;
  text-shadow: 0 0 10px rgba(255, 255, 255, 0.8);
  background-color: rgba(0, 255, 65, 0.1);
  padding: 2px 4px;
}

/* === INPUT BOXES === */
input, select {
  background-color: #001a00;
  color: #00ff41;
  border: 1px solid #00ff41;
  padding: 8px;
  font-family: 'Courier New', monospace;
  font-size: 16px;
  box-shadow: inset 0 0 10px rgba(0, 255, 65, 0.3);
  outline: none;
  
}

select option{
  background-color: #000000;
  color: #00ff41;
}
input:focus, select:focus {
  outline: none !important; /* Força remover o outline */
  border: 1px solid #00ff41 !important; /* Mantém só a borda verde */
  box-shadow: 
    inset 0 0 10px rgba(0, 255, 65, 0.5),
    0 0 10px rgba(0, 255, 65, 0.8);
}
input, select, textarea {
  outline: none !important;
  -webkit-appearance: none;
  -moz-appearance: none;
  appearance: none;
}


/* === TEXTO GLITCHADO === */
.glitch {
  animation: glitch 1s infinite;
}

@keyframes glitch {
  0% { transform: translate(0); }
  20% { transform: translate(-2px, 2px); }
  40% { transform: translate(-2px, -2px); }
  60% { transform: translate(2px, 2px); }
  80% { transform: translate(2px, -2px); }
  100% { transform: translate(0); }
}

/* === SEPARADORES === */
hr {
  border: none;
  border-top: 1px solid #00ff41;
  margin: 20px 0;
  box-shadow: 0 0 5px rgba(0, 255, 65, 0.5);
}

/* === CURSOR PISCANDO === */
tw-story::after {
  content: '█';
  animation: blink 1s infinite;
  margin-left: 5px;
}

@keyframes blink {
  0%, 49% { opacity: 1; }
  50%, 100% { opacity: 0; }
}

/* === EFEITO DE SCANLINE === */
tw-story::before {
  content: '';
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  pointer-events: none;
  background: repeating-linear-gradient(
    0deg,
    rgba(0, 0, 0, 0.15),
    rgba(0, 0, 0, 0.15) 1px,
    transparent 1px,
    transparent 2px
  );
  z-index: 9999;
}

/* === SCROLL BAR === */
\::-webkit-scrollbar {
  width: 12px;
}

\::-webkit-scrollbar-track {
  background: #000000;
  border: 1px solid #00ff41;
}

\::-webkit-scrollbar-thumb {
  background: #00ff41;
  box-shadow: 0 0 10px rgba(0, 255, 65, 0.8);
}

\::-webkit-scrollbar-thumb:hover {
  background: #00cc33;
}

.shake {
  animation: shake 0.5s infinite;
}

@keyframes shake {
  0%, 100% { transform: translateX(0); }
  25% { transform: translateX(-2px); }
  75% { transform: translateX(2px); }
}

.pulse {
  animation: pulse 2s infinite;
}

@keyframes pulse {
  0%, 100% { opacity: 1; }
  50% { opacity: 0.5; }
}





/* === EFEITO SHUDDER (tremor) === */
@-webkit-keyframes shudder {
	50% {
		-webkit-transform: translateX(0.2em);
		transform: translateX(0.2em)
	}
}
@keyframes shudder {
	50% {
		-webkit-transform: translateX(0.2em);
		transform: translateX(0.2em)
	}
}
.shudder {
	-webkit-animation: shudder linear 0.1s 0s infinite;
	animation: shudder linear 0.1s 0s infinite;
	display: inline-block;
}

/* === EFEITO BLINK (piscar) === */
@-webkit-keyframes blinker {
    50% { visibility: hidden; }
}
@keyframes blinker {
    50% { visibility: hidden; }
}
.blink {
    -webkit-animation: blinker 1s step-end infinite;
    animation: blinker 1s step-end infinite;
}

/* === EFEITO BLINK LENTO === */
@keyframes blinker-slow {
    50% { visibility: hidden; }
}
.blink-slow {
    animation: blinker-slow 3s step-end infinite;
}

/* === EFEITO BLINK SUAVE (fade ao invés de corte) === */
@keyframes blinker-fade {
    0%, 100% { opacity: 1; }
    50% { opacity: 0; }
}
.blink-fade {
    animation: blinker-fade 2s ease-in-out infinite;
}

/* === EFEITO BLINK MUITO LENTO === */
@keyframes blinker-veryslow {
    50% { visibility: hidden; }
}
.blink-veryslow {
    animation: blinker-veryslow 5s step-end infinite;
}

/* === EFEITO BLINK IRREGULAR (mais realista) === */
@keyframes blinker-irregular {
    0%, 30% { visibility: visible; }
    35%, 40% { visibility: hidden; }
    45%, 70% { visibility: visible; }
    75%, 78% { visibility: hidden; }
    82%, 100% { visibility: visible; }
}
.blink-irregular {
    animation: blinker-irregular 4s infinite;
}

/* === EFEITO BLUR (embaçado) === */
.blur {
    filter: blur(1px);
    text-shadow: 0 0 5px rgba(0, 255, 65, 0.8);
}

/* === EFEITO RUMBLE (tremor forte) === */
@keyframes rumble {
  0%, 100% { transform: translate(0, 0); }
  25% { transform: translate(-3px, 3px); }
  50% { transform: translate(3px, -3px); }
  75% { transform: translate(-3px, -3px); }
}
.rumble {
  animation: rumble 0.2s infinite;
  display: inline-block;
}

/* === SCANLINE INTENSA === */
.scanline-intense::before {
  content: '';
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  pointer-events: none;
  background: repeating-linear-gradient(
    0deg,
    rgba(0, 0, 0, 0.4),
    rgba(0, 0, 0, 0.4) 1px,
    transparent 1px,
    transparent 2px
  );
  z-index: 9999;
}



/* === BOTÃO DE TOGGLE FLICKER === */
#flicker-toggle {
  position: fixed;
  bottom: 20px;
  right: 20px;
  background-color: rgba(0, 255, 65, 0.2);
  border: 1px solid #00ff41;
  color: #00ff41;
  padding: 10px 15px;
  font-family: 'Courier New', monospace;
  font-size: 12px;
  cursor: pointer;
  z-index: 10000;
  transition: all 0.3s;
}

#flicker-toggle:hover {
  background-color: rgba(0, 255, 65, 0.4);
  box-shadow: 0 0 10px rgba(0, 255, 65, 0.8);
}

/* === DESABILITA FLICKER === */
.no-flicker {
  animation: none !important;
}



