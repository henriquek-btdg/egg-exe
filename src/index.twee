    document.body.appendChild(btn);
    
    var flickerEnabled = true;
    
    btn.addEventListener('click', function() {
        flickerEnabled = !flickerEnabled;
        var html = document.querySelector('html');
        
        if (flickerEnabled) {
            html.classList.remove('no-flicker');
            btn.textContent = 'FLICKER: ON';
        } else {
            html.classList.add('no-flicker');
            btn.textContent = 'FLICKER: OFF';
        }
    });
    
    console.log('Botão de flicker criado!');
});










