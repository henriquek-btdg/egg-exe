// Sistema de flicker aleatório
(function() {
    const html = document.querySelector('html');
    let flickerAtivo = false;
    
    function iniciarFlicker() {
        if (flickerAtivo) return;
        flickerAtivo = true;
        html.style.animation = 'flicker 0.15s infinite';
        
        // Duração do flicker: entre 2 e 4 segundos
        const duracaoFlicker = Math.random() * 1500 + 500;
        
        setTimeout(pararFlicker, duracaoFlicker);
    }
    
    function pararFlicker() {
        flickerAtivo = false;
        html.style.animation = 'none';
        
        // Tempo estável: entre 3 e 7 segundos
        const tempoEstavel = Math.random() * 4000 + 3000;
        
        setTimeout(iniciarFlicker, tempoEstavel);
    }
    
    // Começa estável após 3 segundos
    setTimeout(iniciarFlicker, 3000);
})();




// Toggle de flicker
$(document).ready(function() {
    // Cria botão
    $('body').append('<button id="flicker-toggle">FLICKER: ON</button>');
    
    var flickerEnabled = true;
    
    $('#flicker-toggle').click(function() {
        flickerEnabled = !flickerEnabled;
        
        if (flickerEnabled) {
            $('html').removeClass('no-flicker');
            $(this).text('FLICKER: ON');
        } else {
            $('html').addClass('no-flicker');
            $(this).text('FLICKER: OFF');
        }
    });
});

